pipeline {
    agent any
    stages {
        stage("checkout")
        {
            steps 
            {
                // Get some code from a GitHub repository
              dir  ("/var/lib/jenkins/workspace/frontend@script/fec98651b8459b02d087f63a139849ffdb4bdce6330b8ef82df8f82a6398eeec") 
            }
        
        }
        stage("Build") {
            steps {
                git (url: 'https://github.com/SnehaGhongade/JobPortalfrontend.git') {
                    script {
                        // Build the .NET project
                        sh 'dotnet publish -c Release -o out'
                    }
                }
            }
        }
        stage('Test') {
            steps {
                sh 'make test'
            }
        }
        stage('Deploy') {
            steps {
                sh 'make publish'
            }
        }
    }
}